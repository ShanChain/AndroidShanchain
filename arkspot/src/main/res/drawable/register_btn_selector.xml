<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="false">
        <shape>
            <corners android:radius="20dp"/>
            <solid android:color="@color/register_marjar_color"/>
            <stroke android:width="0.5dp" android:color="@color/register_marjar_color"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="20dp"/>
            <solid android:color="@color/register_marjar_color_s"/>
            <stroke android:width="0.5dp" android:color="@color/register_marjar_color_s"/>
        </shape>
    </item>
</selector>